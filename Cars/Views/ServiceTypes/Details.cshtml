@model Cars.Models.ServiceType
@{
    ViewData["Title"] = "Details";
}
<br />
<h2>Service type details</h2>
<hr />

<div class="form-border">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-1">
                <label asp-for="Name" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input asp-for="Name" disabled class="form-control" />
            </div>
        </div>
    </div>
    <br />
    <div class="form-group">
        @*<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-primary" style="min-width:80px;">Edit</a>*@
        @Html.Partial("_IndividualButtonPartial",
            new IndividualButtonPartial{
                Action="Edit",
                ButtonType="btn-primary",
                Glyph="pencil",
                Text="Edit",
                ServiceId=Model.Id
            })
        <a asp-action="Index" class="btn btn-sm btn-success" style="min-width:80px;">Back to List</a>
    </div>
</div>

